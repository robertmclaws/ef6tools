        <PackageVersion Include="FluentAssertions.Analyzers" Version="0.34.1" />
        <PackageVersion Include="MSTest" Version="3.7.3" />
        <!-- Transitive dependencies pinned to available versions -->
        <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.13.0" />
        <PackageVersion Include="Microsoft.ApplicationInsights" Version="2.23.0" />
        <PackageVersion Include="Microsoft.Data.SqlClient.SNI" Version="6.0.2" />
        <PackageVersion Include="Microsoft.SourceLink.GitHub" Version="8.0.0" />
        <PackageVersion Include="Microsoft.VisualStudio.QualityTools.Common" Version="$(MicrosoftVisualStudioQualityToolsPackageVersion)" />
        <PackageVersion Include="Microsoft.VisualStudio.QualityTools.ExecutionCommon" Version="$(MicrosoftVisualStudioQualityToolsPackageVersion)" />
        <PackageVersion Include="Microsoft.VisualStudio.QualityTools.Vsip" Version="$(MicrosoftVisualStudioQualityToolsPackageVersion)" />
        <PackageVersion Include="Microsoft.VisualStudioEng.MicroBuild.Core" Version="$(MicrosoftVisualStudioEngMicroBuildCorePackageVersion)" />
    </ItemGroup>

</Project>
