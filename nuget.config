			<package pattern="microsoft.bcl.hashcode" />
			<package pattern="microsoft.build.notargets" />
			<package pattern="microsoft.codeanalysis.bannedapianalyzers" />
			<package pattern="microsoft.csharp" />
			<package pattern="microsoft.io.redist" />
			<package pattern="microsoft.net.stringtools" />
			<package pattern="microsoft.netcore.platforms" />
			<package pattern="microsoft.netcore.targets" />
			<package pattern="microsoft.win32.registry" />

			<!-- Tools -->
			<package pattern="codingwithcalvin.vsixsdk" />
			<package pattern="ilrepack.*" />
			<package pattern="krafs.publicizer" />
			<package pattern="newtonsoft.json" />
			<package pattern="ubiety.dns.core" />
