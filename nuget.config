			<package pattern="npgsql" />
			<package pattern="oracle.manageddataaccess" />
			<package pattern="oracle.manageddataaccess.entityframework" />

			<!-- SQL Client and dependencies -->
			<package pattern="microsoft.data.sqlclient" />
			<package pattern="microsoft.data.sqlclient.sni" />
			<package pattern="azure.core" />
			<package pattern="azure.identity" />
			<package pattern="microsoft.identity.client" />
