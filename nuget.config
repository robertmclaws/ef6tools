			<package pattern="bouncycastle.*" />
			<package pattern="google.protobuf" />
			<package pattern="k4os.compression.lz4" />
			<package pattern="k4os.compression.lz4.streams" />
			<package pattern="k4os.hash.xxhash" />
			<package pattern="zstdsharp.port" />

			<!-- System.* packages -->
			<package pattern="system.buffers" />
			<package pattern="system.collections.immutable" />
			<package pattern="system.componentmodel.composition" />
			<package pattern="system.composition" />
			<package pattern="system.composition.*" />
			<package pattern="system.configuration.configurationmanager" />
			<package pattern="system.diagnostics.diagnosticsource" />
